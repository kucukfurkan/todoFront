{
  "consumer": {
    "name": "todo"
  },
  "provider": {
    "name": "todo-backend"
  },
  "interactions": [
    {
      "description": "create item request",
      "providerState": "a todo item is created",
      "request": {
        "method": "POST",
        "path": "/CreateTodo",
        "headers": {
          "Accept": "application/json",
          "Content-Type": "application/json"
        },
        "body": {
          "status": 0,
          "text": "new todo element"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 201,
        "headers": {
          "Content-Type": "application/json"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}